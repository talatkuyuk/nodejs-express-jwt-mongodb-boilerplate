{
  "name": "nodejs-express-jwt-mongodb-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon src/server.js",
    "prod": "cross-env NODE_ENV=production nodemon src/server.js",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --config ./jest.config.json",
    "test:file": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --config ./jest.config.json middleware.authenticate.test.js",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --runInBand --config ./jest.config.json",
    "node:kill": "pkill node",
    "redis:list": "netstat -an | grep 6379 | grep ESTABLISHED",
    "redis:count": "netstat -an | grep 6379 | grep ESTABLISHED | wc -l",
    "redis:kill": "redis-cli CLIENT KILL TYPE normal",
    "redis:stop": "brew services stop redis",
    "redis:restart": "brew services restart redis",
    "prettier": "prettier --check src/**/*.js",
    "prettier:fix": "prettier --write src/**/*.js",
    "prettier:fixfile": "prettier --write src/utils/Utils.js",
    "tsc": "tsc"
  },
